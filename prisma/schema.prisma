generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id   String @id @default(uuid())
  name String

  @@map("users")
}

model Category {
  id   String @id @default(uuid())
  name String

  @@map("categories")
}
